/*按钮*/

QPushButton{
background-color:rgb(70, 70, 70);
border-radius: 10px;
color: white;
}
QPushButton:pressed{
background-color:rgba(70, 70, 70,170);
border-radius: 10px;
color: white;
}
QPushButton:disabled{
background-color:rgb(170, 170, 170);
border-radius: 10px;
color: white;
}
