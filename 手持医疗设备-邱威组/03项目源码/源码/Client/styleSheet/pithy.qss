border-bottom-right-radius: 3px;
min-height:25px;
color :rgb(120, 120, 120);
}

/*ComboBox*/
QComboBox{
background-color:white;
border-top-right-radius: 5px;
border-bottom-right-radius: 5px;
min-height:27px;
color :rgb(150, 150, 150);
}
QComboBox:drop-down{
border-radius:3px;
}
QComboBox::down-arrow{
background-image:url(:/resource/drop-arrow.png);
min-width:28px;
min-height:20px;
background-repeat:no-repeat;
}

/*DateEdit*/
QDateEdit{
background-color:white;
border-top-right-radius: 5px;
border-bottom-right-radius: 5px;
min-height:27px;
color :rgb(150, 150, 150);
}


/*radioButton*/
QRadioButton::indicator {
    width: 40px;
    height: 40px;
}
QRadioButton#DoctorButton::indicator::unchecked {
image: url(:/resource/unchecked2.png);
}
QRadioButton#DoctorButton::indicator::checked {
image: url(:/resource/checked2.png);
}
QRadioButton#PatientButton::indicator::unchecked {
image: url(:/resource/unchecked1.png);
}
QRadioButton#PatientButton::indicator::checked {
image: url(:/resource/checked1.png);
}

